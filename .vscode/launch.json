            "executable": "./build/gk.elf",
            "request": "attach",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "jlink",
            "device": "STM32H747BI_M7",
            "serverpath": "C:\\Program Files\\SEGGER\\JLink\\JLinkGDBServerCL.exe",
            //"rtos": "c:\\Program Files\\SEGGER\\JLink\\GDBServer\\RTOSPlugin_FreeRTOS.dll",
